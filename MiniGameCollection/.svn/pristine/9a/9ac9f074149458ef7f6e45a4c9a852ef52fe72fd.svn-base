package minigame;

public class Mole_Information {
	String userName;
	int moleScore;
	String moleLevel;
	
	public Mole_Information(String userName, int moleScore, String moleLevel) {
		this.userName = userName;
		this.moleScore = moleScore;
		this.moleLevel = moleLevel;
	}
	
	public String getUserName() {
		return userName;
	}
	public void setUserName(String userName) {
		this.userName = userName;
	}
	public int getMoleScore() {
		return moleScore;
	}
	public void setMoleScore(int moleScore) {
		this.moleScore = moleScore;
	}
	public String getMoleLevel() {
		return moleLevel;
	}
	public void setMoleLevel(String moleLevel) {
		this.moleLevel = moleLevel;
	}
}